<!DOCTYPE html>
<html>
<head>
	<title>User List</title>
</head>
<body>
	<h2>Requseted info. List</h2>
	<a href="/home">back</a> |
	<a href="/logout">LogOut</a><br>

	<table border="1">
		
		<tr>
			<td>ID</td>
			<td>Country</td>
			<td>Place</td>
			<td>Cost</td>
			<td>Dicission</td>
		
		</tr>
	<% for(var i=0; i < userlist.length; i++){ %>
		<tr>
			<td><%= userlist[i].id %></td>
			<td><%= userlist[i].country %></td>
			<td><%= userlist[i].place %></td>
			<td><%= userlist[i].cost %></td>
			<td>
				<a href="/home/req_info/<%= userlist[i].id %>">Approve</a> or
				<a href="/home/req_info/delete/<%= userlist[i].id %>">Deny</a>

			</td>
			
		</tr>
		<% } %>
	</table>
</body>
</html>